{
  "name": "running-store",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/running_store",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd ../../ && nuxt prepare && nuxt build",
        "cwd": "apps/running_store"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt dev",
        "cwd": "apps/running_store"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt dev",
        "cwd": "apps/running_store"
      }
    },
    "generate": {
      "executor": "@nx/nuxt:generate",
      "options": {}
    },
    "preview": {
      "executor": "@nx/nuxt:preview",
      "options": {}
    },
    "postinstall": {
      "executor": "@nx/nuxt:prepare",
      "options": {}
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "apps/running_store/**/*.{ts,tsx,vue}",
          "apps/running_store/package.json"
        ],
        "ignorePath": "../../.eslintignore"
      }
    },
    "lint:fix": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "apps/running_store/**/*.{ts,tsx,vue}",
          "apps/running_store/package.json"
        ],
        "fix": true,
        "ignorePath": "../../.eslintignore"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "config": "apps/running_store/vitest.config.ts"
      }
    }
  },
  "tags": []
}
